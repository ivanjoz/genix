{
	"name": "genix-frontend",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"workspaces": [
		"./pkg-store"
	],
	"scripts": {
		"dev": "bun scripts/dev-all.js",
		"dev:main": "vite dev --force --port 3570",
		"dev:store": "cd pkg-store && bun run dev",
		"serve": "bun run serve:build",
		"serve:build": "bun x serve ./build -l 3000",
		"prepare": "svelte-kit sync || echo ''",
		"build": "bun scripts/build-all.js",
		"build:main": "vite build",
		"build:store": "cd pkg-store && bun run build",
		"publish": "bun run build && bun postbuild.js --publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"prebuild:store": "bun scripts/thumbhash-prebuild.js"
	},
	"overrides": {
		"rolldown": "^1.0.0-beta.58"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^24.10.0",
		"http-proxy": "^1.18.1",
		"http-proxy-middleware": "^3.0.5",
		"rolldown": "^1.0.0-beta.58",
		"sharp": "^0.34.3",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"svelte-infinitable": "^0.0.14",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "npm:rolldown-vite@latest"
	},
	"dependencies": {
		"@huggingface/transformers": "^3.8.1",
		"@humanspeak/svelte-virtual-list": "^0.3.5",
		"@jsquash/avif": "^2.1.1",
		"@mediapipe/tasks-genai": "^0.10.25",
		"@mlc-ai/web-llm": "^0.2.80",
		"@xenova/transformers": "^2.17.2",
		"axios": "^1.13.2",
		"embla-carousel-svelte": "^8.6.0",
		"esbuild": "^0.25.12",
		"mime-types": "^3.0.1",
		"notiflix": "^3.2.8",
		"onnxruntime-web": "^1.23.2",
		"peerjs": "^1.5.5",
		"rolldown-vite": "^7.3.0",
		"roosterjs": "^9.41.0",
		"svelte-awesome-color-picker": "^4.1.0",
		"svelte-lexical": "^0.6.3",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"thumbhash": "^0.1.1"
	}
}
