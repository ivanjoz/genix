{
	"name": "frontend2",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --force",
		"serve": "vite build && vite preview --port 3000",
		"prepare": "svelte-kit sync || echo ''",
		"build": "vite build && node postbuild",
		"publish": "vite build && node postbuild --publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"overrides": {
		"rolldown": "^1.0.0-beta.58"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@latest",
			"rolldown": "^1.0.0-beta.58"
		}
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/typography": "^0.5.18",
		"@tailwindcss/vite": "^4.1.13",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^24.10.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"svelte-infinitable": "^0.0.14",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "npm:rolldown-vite@latest",
		"rolldown": "^1.0.0-beta.58"
	},
	"dependencies": {
		"@huggingface/transformers": "^3.8.1",
		"@humanspeak/svelte-virtual-list": "^0.3.5",
		"@jsquash/avif": "^2.1.1",
		"@mediapipe/tasks-genai": "^0.10.25",
		"@mlc-ai/web-llm": "^0.2.80",
		"@xenova/transformers": "^2.17.2",
		"axios": "^1.13.2",
		"esbuild": "^0.25.12",
		"mime-types": "^3.0.1",
		"notiflix": "^3.2.8",
		"onnxruntime-web": "^1.23.2",
		"rolldown-vite": "^7.3.0",
		"roosterjs": "^9.41.0",
		"svelte-awesome-color-picker": "^4.1.0",
		"svelte-lexical": "^0.6.3",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1"
	}
}